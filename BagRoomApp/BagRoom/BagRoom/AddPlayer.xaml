<Window x:Class="BagRoom.AddPlayer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BagRoom"
        mc:Ignorable="d"
        Title="AddPlayer" Height="450" Width="800">
    <Grid>
        <TextBox Name="fName" HorizontalAlignment="Left" Height="23" Margin="336,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="First Name" HorizontalAlignment="Left" Margin="264,62,0,0" VerticalAlignment="Top"/>
        <Label Content="Last Name" HorizontalAlignment="Left" Margin="265,84,0,0" VerticalAlignment="Top"/>
        <TextBox Name="lName" HorizontalAlignment="Left" Height="23" Margin="336,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Bag Number" HorizontalAlignment="Left" Margin="254,110,0,0" VerticalAlignment="Top"/>
        <TextBox Name="bagNum" HorizontalAlignment="Left" Height="23" Margin="336,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Click="insertPlayer" Content="Submit" HorizontalAlignment="Left" Margin="359,138,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
